# Python/Django .gitignore

# Python bytecode
__pycache__/
*.py[cod]
*$py.class

# Django
*.log
db.sqlite3
media/

# Distribution / packaging
dist/
build/
*.egg-info/

# Virtual environments
venv/
env/
ENV/

# Environment variables
.env

# IDE specific files
.idea/
.vscode/
*.swp
*.swo

# OS specific files
.DS_Store
Thumbs.db

# Static files collected
staticfiles/

# Local development settings
local_settings.py

# Test coverage
htmlcov/
.coverage
.coverage.*

# Support attachments (may contain sensitive data)
support_attachments/

# Temporary files
*.bak
*.tmp

# Ignore sensitive information that might be in .env files
*.env